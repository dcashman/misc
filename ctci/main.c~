#include <stdio.h>
extern int find(int array[], int size);

int main(int argc, char* argv[]){

  int arrayOdd[4][5]={{4,6,7,8,9},{4,5,7,8,9},{4,5,6,8,9},{4,5,6,7,9}};
  int arrayEven[4][6]={{4,6,7,8,9,10},{4,5,7,8,9,10},{4,5,6,8,9,10},{4,5,6,7,9,10}};
  int resultOdd, resultEven;

  //test with odd number-array
  for(int i=0; i<4; i++){
    resultOdd=find(arrayOdd[i],5);
    resultEven=find(arrayEven[i],6);
    if(resultOdd!=i+5){
      printf("Error with Odd iteration\t%d\n",i);
    }else if(resultEven!=i+5){
      printf("Error with Even iteration\t%d\n",i);
    }else{
      printf("Result%d\tOdd:\t%d\tEven:\t%d\n",i,resultOdd, resultEven);
    }
  }

  return 0;
}
